    "definitions": {
        "dto.LoginInput": {
            "type": "object",
            "required": [
                "email",
                "password"
            ],
            "properties": {
                "email": {
                    "type": "string",
                    "example": "ian@gmail.com"
                },
                "password": {
                    "type": "string",
                    "example": "P@ssW0rd"
                }
            }
        },
        "dto.RegisterInput": {
            "type": "object",
            "required": [
                "email",
                "name",
                "password"
            ],
            "properties": {
                "email": {
                    "type": "string",
                    "example": "ian@gmail.com"
                },
                "name": {
                    "type": "string",
                    "example": "Rian"
                },
                "password": {
                    "type": "string",
                    "minLength": 6,
                    "example": "P@ssW0rd"
                }
            }
        },
        "dto.RoleInput": {
            "type": "object",
            "required": [
                "name"
            ],
            "properties": {
